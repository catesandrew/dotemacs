# -*- coding: utf-8; mode: snippet; require-final-newline: nil -*-
# name: date to string
# key: date2str
# group: other
# binding: direct-keybinding
# --
${1:var date2str} = function(d) {
  function fnum(n) {
    return n <= 9 ? '0' + n : '' + n
  }
  return d.getFullYear() + '-' +
    fnum(d.getMonth() + 1) + '-' +
    fnum(d.getDate())
}